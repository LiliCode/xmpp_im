project 'xmpp_im.xcodeproj'

platform :ios, '8.0'

target 'xmpp_im' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
   use_frameworks!

 	pod 'XMPPFramework'
	pod 'MBProgressHUD'
  	pod 'AFNetworking'
	pod 'FMDB'
	pod 'SDWebImage'
	pod 'HYBImageCliped'

  target 'xmpp_imTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'xmpp_imUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
